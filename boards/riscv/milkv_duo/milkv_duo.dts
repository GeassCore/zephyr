/dts-v1/;

#include <milkv/cv180x_base.dtsi>

/ {
	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};

	ram0: ram0@80000000 {
		compatible = "memory";
		reg = <0x0 0x80000000 0x0 0x3f40000>;
		reg-names = "mem";
	};
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
};